{
  "private": false,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "name": "typorama",
  "version": "0.0.144",
  "description": "data type system for javascript",
  "main": "dist/src",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wix/typorama"
  },
  "scripts": {
    "build:src": "rm -rf ./dist/src && babel ./src/ -d ./dist/src/ --modules umd --source-maps",
    "build:test": "rm -rf ./dist/test && babel ./test/ -d ./dist/test/ --modules umd --source-maps",
    "build:testkit": "babel ./test-kit/ -d ./dist/test-kit/ --modules umd --source-maps",
    "build:webpack": "webpack",
    "test:node": "mocha --reporter $npm_config_reporter ./dist/test",
    "test:browser": "karma start --single-run",
    "pretest": "npm run build:test && npm run build:testkit && npm run build:webpack",
    "test": "npm run test:browser && npm run test:node",
    "start": "webpack-dev-server --progress --hot --inline --no-colors"
  },
  "author": "core3",
  "license": "c",
  "devDependencies": {
    "babel": "5.4.7",
    "babel-core": "5.4.7",
    "babel-loader": "5.1.3",
    "bluebird": "2.9.34",
    "chai": "2.3.0",
    "immutable": "3.7.3",
    "karma": "0.13.9",
    "karma-chrome-launcher": "0.2.0",
    "karma-mocha": "0.2.0",
    "karma-teamcity-reporter": "0.2.0",
    "mocha": "2.2.5",
    "mocha-loader": "0.7.1",
    "mocha-teamcity-reporter": "0.0.4",
    "node-libs-browser": "0.5.2",
    "react": "0.13.3",
    "sinon": "git://github.com/cjohansen/Sinon.JS.git#b672042043517b9f84e14ed0fb8265126168778a",
    "sinon-chai": "2.8.0",
    "webpack": "1.9.10",
    "webpack-dev-server": "1.9.0"
  },
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "lodash": "3.9.3",
    "gopostal": "0.0.39"
  }
}
