{
  "private": true,
  "name": "typorama",
  "version": "0.0.10",
  "description": "",
  "main": "dist/src",
  "repository": {
    "type": "git",
    "url": "wix/typorama"
  },
  "scripts": {
    "watch": "babel -w ./src/ -d ./dist/src/ --modules umd --source-maps",
    "build:test": "babel ./test/ -d ./dist/test/ --modules umd --source-maps",
    "build:testkit": "babel ./test-kit/ -d ./dist/test-kit/ --modules umd --source-maps",
    "build:src": "babel ./src/ -d ./dist/src/ --modules umd --source-maps",
    "build" : "npm run build:src && npm run build:test && npm run build:testkit",
    "postinstall" : "npm run build:src",
    "test": "mocha --compilers js:babel/register --reporter list",
    "start": "npm run webtest",
    "webtest": "webpack-dev-server --progress --hot --inline --no-colors",
    "webbuild": "webpack --progress -p --colors",
    "prepublish": "npm run test && npm run build:src"
  },
  "author": "core3",
  "license": "c",
  "devDependencies": {
    "babel": "^4.7.16",
    "babel-core": "^4.7.16",
    "babel-loader": "^4.2.0",
    "babel-runtime": "^4.7.16",
    "bluebird": "^2.9.14",
    "chai": "^2.1.2",
    "immutable": "^3.6.4",
    "mocha": "^2.2.1",
    "mocha-loader": "^0.7.1",
    "react": "^0.13.1",
    "webpack": "^1.7.3",
    "sinon": "git://github.com/cjohansen/Sinon.JS#b672042043517b9f84e14ed0fb8265126168778a"
  },
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "lodash": "^3.5.0"
  }
}
