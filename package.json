{
  "private": false,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "name": "typorama",
  "version": "0.0.299",
  "description": "data type system for javascript",
  "main": "dist/src",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wix/typorama"
  },
  "scripts": {
    "build:src": "rm -rf ./dist/src && babel ./src/ -d ./dist/src/ --source-maps",
    "build:test": "rm -rf ./dist/test && babel ./test/ -d ./dist/test/ --source-maps",
    "build:testkit": "babel ./test-kit/ -d ./dist/test-kit/ --source-maps",
    "build:webpack": "webpack",
    "test:node": "mocha --reporter mocha-env-reporter ./dist/test.bundle.js",
    "test:browser": "karma start --single-run",
    "pretest": "npm run build:test && npm run build:testkit && npm run build:webpack",
    "test": "npm run test:browser && npm run test:node",
    "start": "webpack-dev-server --progress --hot --inline --no-colors"
  },
  "author": "core3",
  "license": "c",
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.6.0",
    "babel-plugin-transform-es2015-modules-umd": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-react": "6.5.0",
    "chai": "3.5.0",
    "immutable": "3.7.6",
    "karma": "0.13.22",
    "karma-chrome-launcher": "0.2.3",
    "karma-mocha": "0.2.2",
    "karma-teamcity-reporter": "0.2.2",
    "mocha": "2.4.5",
    "mocha-loader": "0.7.1",
    "mocha-env-reporter": "1.0.2",
    "react": "0.14.8",
    "sinon": "git://github.com/cjohansen/Sinon.JS.git#b672042043517b9f84e14ed0fb8265126168778a",
    "sinon-chai": "2.8.0",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "lodash": "4.9.0",
    "escalate": "0.0.76"
  },
  "babel": {
    "presets": [
      "es2015-loose",
      "react"
    ],
    "plugins": [
      "transform-es2015-modules-umd",
      "transform-class-properties"
    ]
  }
}
