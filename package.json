{
  "private": false,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "name": "typorama",
  "version": "0.0.252",
  "description": "data type system for javascript",
  "main": "dist/src",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wix/typorama"
  },
  "scripts": {
    "build:src": "rm -rf ./dist/src && babel ./src/ -d ./dist/src/ --modules umd --source-maps --loose all",
    "build:test": "rm -rf ./dist/test && babel ./test/ -d ./dist/test/ --modules umd --source-maps --loose all",
    "build:testkit": "babel ./test-kit/ -d ./dist/test-kit/ --modules umd --source-maps --loose all",
    "build:webpack": "webpack",
    "test:node": "mocha --reporter $npm_config_reporter ./dist/test.bundle.js",
    "test:browser": "karma start --single-run",
    "pretest": "npm run build:test && npm run build:testkit && npm run build:webpack",
    "test": "npm run test:browser && npm run test:node",
    "start": "webpack-dev-server --progress --hot --inline --no-colors"
  },
  "author": "core3",
  "license": "c",
  "devDependencies": {
    "babel": "5.8.35",
    "babel-core": "5.8.35",
    "babel-loader": "5.4.0",
    "bluebird": "3.0.5",
    "chai": "3.5.0",
    "immutable": "3.7.6",
    "karma": "0.13.19",
    "karma-chrome-launcher": "0.2.2",
    "karma-mocha": "0.2.1",
    "karma-teamcity-reporter": "0.2.1",
    "mocha": "2.4.5",
    "mocha-loader": "0.7.1",
    "mocha-teamcity-reporter": "1.0.0",
    "react": "0.14.7",
    "sinon": "git://github.com/cjohansen/Sinon.JS.git#b672042043517b9f84e14ed0fb8265126168778a",
    "sinon-chai": "2.8.0",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.14.1"
  },
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "lodash": "3.10.1",
    "gopostal": "0.0.60"
  }
}
